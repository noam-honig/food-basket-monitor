{
  "name": "radweb-starterkit",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "gc": "ng generate component --skipTests=true",
    "start": "npm run server:execute",
    "ng-serve": "ng serve",
    "start-ip-10.0.0.6": "ng serve --host 10.0.0.6",
    "build": "npm run server:build && npm run ng-build",
    "ng-build": "ng build  --prod",
    "server:build": "tsc -p tsconfig.server.json",
    "server:build-watch": "tsc -p tsconfig.server.json --watch",
    "server:execute": "node dist-server/server/server.js",
    "server:debug": "node --inspect --debug-brk dist-server/server/server.js",
    "RunSomething": "node --inspect --debug-brk dist-server/server/doSomething.js",
    "publish-to-heroku": "git push fbd-monitor master -f",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.9",
    "@angular/cdk": "^7.3.4",
    "@angular/common": "^7.2.9",
    "@angular/compiler": "^7.2.9",
    "@angular/core": "^11.0.5",
    "@angular/forms": "^7.2.9",
    "@angular/http": "^7.2.9",
    "@angular/material": "^7.3.4",
    "@angular/platform-browser": "^7.2.9",
    "@angular/platform-browser-dynamic": "^7.2.9",
    "@angular/router": "^7.2.9",
    "@auth0/angular-jwt": "^3.0.0",
    "body-parser": "^1.18.3",
    "bootstrap3": "^3.3.5",
    "chart.js": "^2.7.3",
    "compression": "^1.7.3",
    "copy-to-clipboard": "^3.0.8",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-force-https": "^1.0.0",
    "express-sse": "^0.5.0",
    "hammerjs": "^2.0.8",
    "jsonwebtoken": "^8.5.1",
    "ng2-charts": "^1.6.0",
    "ngx-bootstrap": "^3.1.1",
    "node-fetch": "^2.2.1",
    "password-hash": "^1.2.2",
    "path": "^0.12.7",
    "pg": "^7.6.1",
    "reflect-metadata": "^0.1.12",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "uuid": "^3.3.2",
    "zone.js": "~0.8.26",
